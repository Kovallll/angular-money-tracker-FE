    "@angular/forms": "^20.1.0-next",
    "@angular/material": "^20.2.0",
    "@angular/platform-browser": "^20.1.0-next",
    "@angular/platform-browser-dynamic": "^20.3.7",
    "@angular/router": "^20.1.0-next",
    "@primeuix/styled": "^0.7.4",
    "@primeuix/themes": "^1.2.5",
    "@primeuix/utils": "^0.6.1",
    "@tanstack/angular-query-experimental": "^5.90.7",
    "chart.js": "^4.5.1",
    "dayjs": "^1.11.18",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "ng2-charts": "^8.0.0",
    "prettier": "^3.6.2",
    "primeicons": "^7.0.0",

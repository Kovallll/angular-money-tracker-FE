{
  "name": "first-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:spa": "ng serve --configuration spa",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"src/**/*.{ts,html,scss,css,json,md}\"",
    "prepare": "husky",
    "build:single-spa:first-app": "ng build first-app --configuration production",
    "serve:single-spa:first-app": "ng s --project first-app --disable-host-check --port 4200 --live-reload false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.4",
